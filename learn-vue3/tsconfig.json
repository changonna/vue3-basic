{
	"compilerOptions": {
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"types": ["vite/client"]
	},
	"references": [
		{
			"path": "./tsconfig.vite-config.json"
		}
	],
	"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
	"extends": "@vue/tsconfig/tsconfig.json"
}
